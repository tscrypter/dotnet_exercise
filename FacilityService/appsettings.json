{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Spring": {
    "Application": {
      "Name": "facilityApi"
    }
  },
  "Eureka": {
    "Client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "ShouldFetchRegistry": false,
      "ValidateCertificates": false
    },
    "Instance": {
      "port": 5000,
      "InstanceId": "${spring:application:name}"
    }
  },
  "AllowedHosts": "*"
}
