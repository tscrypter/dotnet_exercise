{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator":{
        "Exposure": {
          "Include": [ "*" ],
          "Exclude": [ "env", "refresh"]
        }
      }
    }
  },
  "Eureka": {
    "Instance": {
      "StatusPageUrlPath": "/actuator/info"
    },
    "Client": {
      "ServiceUrl": "http://eureka:8761/eureka"
    }
  },
  "AllowedHosts": "*",
  "Spring": {
    "Application": {
      "Name": "FacilityApi"
    }
  }
}
